{
  "openapi": "3.0.1",
  "info": {
    "title": "EventsService API",
    "description": "Microservicio de eventos (zonas, asientos, escenarios).",
    "version": "v1"
  },
  "paths": {
    "/api/events/{eventId}/zonas/{zonaId}/asientos": {
      "post": {
        "tags": [
          "Asientos"
        ],
        "summary": "Crea un asiento en la zona indicada del evento.",
        "description": "Lanza `DomainException` cuando los datos son inválidos y `NotFoundException` si la zona no existe.",
        "parameters": [
          {
            "name": "eventId",
            "in": "path",
            "description": "Id del evento",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "zonaId",
            "in": "path",
            "description": "Id de la zona",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "requestBody": {
          "description": "Datos del asiento a crear",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CrearAsientoDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CrearAsientoDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CrearAsientoDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Asiento creado",
            "content": {
              "application/json": {
                "schema": { }
              }
            }
          },
          "404": {
            "description": "Evento/Zona no existe",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          },
          "422": {
            "description": "Validación de dominio fallida",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Asientos"
        ],
        "summary": "Lista los asientos de una zona.",
        "parameters": [
          {
            "name": "eventId",
            "in": "path",
            "description": "Id del evento",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "zonaId",
            "in": "path",
            "description": "Id de la zona",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Listado de asientos",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/AsientoDto"
                  }
                }
              }
            }
          },
          "404": {
            "description": "Zona no existe",
